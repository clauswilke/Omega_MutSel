#NEXUS
begin taxa;
	dimensions ntax=4;
	taxlabels
	t4
	t1
	t3
	t2
;
end;

begin trees;
	tree tree_1 = [&R] ((t4:0.01,t1:0.01):0.01,(t3:0.01,t2:0.01):0.01);
end;

